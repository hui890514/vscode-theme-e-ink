        "string.quoted.double",
        "string.quoted.triple",
        "string.quoted.other",
        "string.unquoted",
        "string.interpolated",
        "string.regexp",
        "string.other",
        "text.html.derivative",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#1CB01C",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity",
        "entity.name",
        "entity.name.function",
        "entity.name.type",
        "entity.name.tag",
        "entity.name.section",
        "entity.other",
        "entity.other.inherited-class",
        "entity.other.attribute-name",
        "markup.bold",
        "fenced_code.block.language.markdown"
      ],
      "settings": {
        "foreground": "#FF79C6",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "invalid",
        "invalid.illegal",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#FF5555",
        "fontStyle": ""
      }
    },
    {
      "scope": [
