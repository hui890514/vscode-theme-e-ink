        "comment.block",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#2AA198",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "constant",
        "constant.numeric",
        "constant.character",
        "constant.character.escape",
        "constant.language",
        "constant.other",
        "markup.underline",
        "markup.underline.link",
